{
  "name": "virgil-crypto",
  "version": "3.0.0-alpha.14",
  "main": "dist/virgil-crypto.cjs.js",
  "browser": {
    "./dist/virgil-crypto.cjs.js": "./dist/virgil-crypto.browser.cjs.js",
    "./dist/virgil-crypto.es.js": "./dist/virgil-crypto.browser.es.js",
    "./src/lib/pythia-node.ts": "./src/lib/pythia-browser.ts"
  },
  "module": "dist/virgil-crypto.es.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "scripts/download-node-addon.js",
    "scripts/helpers/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/VirgilSecurity/virgil-crypto-javascript"
  },
  "author": "Virgil Security Inc. <support@virgilsecurity.com>",
  "keywords": [
    "security",
    "elliptic",
    "elliptic curve",
    "virgil",
    "virgilsecurity",
    "encryption",
    "crypto"
  ],
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">= 4.5.0 < 5 || >= 6"
  },
  "scripts": {
    "clean": "rimraf .rpt2_cache dist",
    "build:main": "node scripts/rollup/build.js",
    "build:pythia": "cross-env PYTHIA=1 node scripts/rollup/build.js",
    "build": "npm-run-all clean build:*",
    "preversion": "npm-run-all update:* test",
    "version": "npm run build",
    "test:node": "cross-env TS_NODE_FILES=true mocha --require chai/register-assert --require ts-node/register \"src/tests/**/*.test.ts\"",
    "test:browser": "karma start",
    "test": "npm-run-all test:*",
    "postinstall": "npm run update:node",
    "docs": "typedoc src",
    "update:node": "node scripts/download-node-addon.js",
    "update:asmjs": "node scripts/download-asmjs.js && node scripts/download-pythia-asmjs.js"
  },
  "dependencies": {
    "node-fetch": "^2.1.2",
    "tar": "^4.4.2",
    "tempy": "^0.2.1",
    "yauzl": "^2.9.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^10.5.2",
    "buffer-es6": "^4.9.3",
    "builtin-modules": "^3.0.0",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^6.0.0",
    "lint-staged": "^7.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.0.5",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.4.4",
    "rimraf": "^2.6.1",
    "rollup": "^0.63.4",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-node-globals": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.16.1",
    "rollup-plugin-uglify": "^4.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.7.0",
    "tslint-config-prettier": "^1.1.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.6.2"
  },
  "virgil-crypto-lib-version": "2.4.6"
}
